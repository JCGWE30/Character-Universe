<Pane fx:id="encompassPane" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lemees.fxgrid.GridController">
   <children>
      <ScrollPane prefHeight="499.0" prefWidth="150.0">
         <content>
            <VBox fx:id="creatureScroll" prefHeight="68.0" prefWidth="598.0" />
         </content>
      </ScrollPane>
      <ScrollPane fx:id="eventScrollPane" layoutY="526.0" prefHeight="75.0" prefWidth="600.0">
         <content>
            <VBox fx:id="eventScroll" prefHeight="68.0" prefWidth="598.0" />
         </content>
      </ScrollPane>
      <AnchorPane fx:id="eventAnchor" layoutX="150.0" prefHeight="525.0" prefWidth="450.0" />
      <AnchorPane layoutY="499.0" prefHeight="30.0" prefWidth="150.0">
         <children>
            <Button fx:id="pauseButton" layoutY="1.0" mnemonicParsing="false" text="Pause" />
            <Button fx:id="fastButton" layoutX="62.0" layoutY="1.0" lineSpacing="5.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="22.0" text="^">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <Button fx:id="slowButton" layoutX="89.0" layoutY="1.0" mnemonicParsing="false" prefHeight="14.0" prefWidth="2.0" text="v" />
            <Label fx:id="speedDisplay" layoutX="116.0" layoutY="5.0" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" text="x1" textAlignment="CENTER" textOverrun="CLIP" />
         </children>
      </AnchorPane>
   </children>
</Pane>
